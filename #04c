/*Fa√ßa um programa que preencha uma matriz 3x3 com valores inteiros entre [1..9]*/
#include <stdio.h>
int main() {
    int matriz[3][3], i, j;
    for (i = 0; i < 3; i++) {
      for (j = 0; j < 3; j++) {
        do {
          scanf("%d", &matriz[i][j]);
        } while(matriz[i][j] < 0 || matriz[i][j] > 9 );
      }
    }
    for (i = 0; i < 3; i++) {
      for (j = 0; j < 3; j++) {
        printf("%d", matriz[i][j]);
      }
    printf ("\n");
    }
    return 0;
}
  

#include <stdio.h>

int main() {
	int X;
	scanf("%d", &X);
	if (X == 1){
       printf("January\n");}
    else{
		if (X == 2){
			printf("February\n");}
		if (X == 3){
			printf("March\n");}
		if (X == 4){
			printf("April\n");}
		if (X == 5){
			printf("May\n");}
		if (X == 6){
			printf("June\n");}
		if (X == 7){
			printf("July\n");}
		if (X == 8){
			printf("August\n");}
		if (X == 9){
			printf("September\n");}
		if (X == 10){
			printf("October\n");}
		if (X == 11){
			printf("November\n");}
		if (X == 12){
          printf("December\n");}
		}
	return 0;
}

#include <stdio.h>

int main() 
{
	int valortotal, valor, troco, cem , cinq , vint , dez , cinco , dois , um;
	scanf("%d", &valortotal);
	valor = valortotal;
	cem = valor / 100;
	valor = valor - (cem *100);
	cinq = valor / 50;
	valor = valor - (cinq * 50);
	vint = valor / 20;
	valor = valor - (vint * 20);
	dez = valor /10;
	valor = valor -(dez * 10);
	cinco = valor / 5;
	valor = valor - (cinco * 5);
	dois = valor / 2;
	valor = valor - (dois * 2);
	um = valor / 1;
	printf("%d\n",valortotal);
	printf("%d nota(s) de R$ 100,00\n", cem);
	printf("%d nota(s) de R$ 50,00\n", cinq);
	printf("%d nota(s) de R$ 20,00\n", vint);
	printf("%d nota(s) de R$ 10,00\n", dez);
	printf("%d nota(s) de R$ 5,00\n", cinco);
	printf("%d nota(s) de R$ 2,00\n", dois);
	printf("%d nota(s) de R$ 1,00\n", um);
return 0;
}

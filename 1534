#include <stdio.h>

int main() {
  int N;
  while (scanf("%i", &N) != EOF) {
    int l, c;
    for (l = 0; l < N; l++) {
      for (c = 0; c < N; c++) {
        if (l + c == N - 1) 
          { printf("2"); }
        else 
          if (l == c) 
            { printf("1"); }
          else 
            { printf("3"); }
        }
        printf("\n");
    }
  }
    return 0;
}
